<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PruebasEndPoint">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


    <Button
        android:id="@+id/btnPeticionBin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button Bin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnPeticionAccesoUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button AccesoUsuario"
         />

    <Button
        android:id="@+id/btnPeticionAcumulaPuntos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button AcumulaPuntosPuntada"

        />

    <Button
        android:id="@+id/btnPeticionGeneraTicket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Button GeneraTicket"
       />

    <Button
        android:id="@+id/btnPeticionFormasPago"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Button FormasPago"
         />

    <Button
        android:id="@+id/btnPeticionEmpleado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Button Empleado"
         />

    <Button
        android:id="@+id/btnPeticionProductosProcedencia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button ProductosProcedencia"
         />

    <Button
        android:id="@+id/btnPeticiongetPostFinalizaVenta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button PostFinalizaVenta"
        />

        <Button
            android:id="@+id/btnPeticionTicketPendienteCobro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Button TicketPendienteCobro"
             />

            <Button
                android:id="@+id/btnPeticionAutorizaDespacho"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Button AutorizaDespacho" />

        </LinearLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>